<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="persistence-data">
		<class>com.printmanagement.entity.UserEntity</class>
		<class>com.printmanagement.entity.RoleEntity</class>
		<class>com.printmanagement.entity.CustomerEntity</class>
		<class>com.printmanagement.entity.CustomerTypeEntity</class>
		<class>com.printmanagement.entity.ItemEntity</class>
		<class>com.printmanagement.entity.ItemTypeEntity</class>
		<class>com.printmanagement.entity.PriceListEntity</class>
		<class>com.printmanagement.entity.OrdersEntity</class>
		<class>com.printmanagement.entity.MaterialEntity</class>
		<class>com.printmanagement.entity.MaterialTypeEntity</class>
		<class>com.printmanagement.entity.PurchaseMaterialEntity</class>
		<class>com.printmanagement.entity.PurchaseMaterialDetailEntity</class>
		<class>com.printmanagement.entity.CostEntity</class>
		<properties>

			<property name="hibernate.show_sql" value="true" />
			 <property name="logging.level.org.hibernate.SQL" value="debug"/>
 			<property name="logging.level.org.hibernate.type.descriptor.sql" value="trace"/>
		</properties>

	</persistence-unit>
</persistence>

